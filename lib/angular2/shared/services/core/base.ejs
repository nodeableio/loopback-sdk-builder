/**
* @module BaseLoopBackApi
* @author Jonathan Casarrubias <@johncasarrubias> <github:jonathan-casarrubias>
* @author Nikolay Matiushenkov <https://github.com/mnvx>
* @license MIT
* @description
* Abstract class that will be implemented in every custom service automatically built
* by the sdk builder.
* It provides the core functionallity for every API call, either by HTTP Calls or by
* WebSockets.
**/
@Injectable()
export abstract class BaseLoopBackApi<T> {

  protected path: string;
  protected model: any;
  protected event;
  protected rooms:string[] = [];

  protected items$:Subject<T[]>;
  protected item$:Subject<T>;
  protected items:T[];
  protected item:T;

  private cache = {};

  private reset$:Subject<never> = new Subject();

  constructor(
    @Inject(SocketService) protected socket:SocketService,
    <%- buildServiceDI(isIo) %>

  ) {
    this.model = this.models.get(this.getModelName());
    const room = this.getModelName().toLowerCase();
    this.event = this.socket.init(this, [...this.rooms, room]);

    this.event.onEvent(`${room}.update`).subscribe(this.onUpdate);
    this.event.onEvent(`${room}.create`).subscribe(this.onNew);
    this.event.onEvent(`${room}.delete`).subscribe(this.onDelete);
  }

    protected onUpdate = data => {
        const updated = { ...data };

        const index = this.items.findIndex(entry => entry['id'] === data.id);

        if (index > -1) {
            this.items[index] = updated;
            this.items$.next(this.items);
        }

        if (this.item['id'] === data.id) {
            this.item = updated;
            this.item$.next(this.item);
        }
    };

    protected onNew = data => {
        this.items.push(data);
        this.items$.next(this.items);
    };

    protected onDelete = data => {
        const index = this.items.findIndex(entry => entry['id'] === data.id);

        if (index > -1) {
            this.items.splice(index, 1);
            this.items$.next(this.items);
        }

        if (this.item['id'] === data.id) {
            this.item = null;
            this.item$.next(this.item);
        }
    };

    public resetCache = (data:T[] = []) => {
        this.reset$.next();
        this.items = data;
        this.items$ = null;
        this.item = null;
        this.item$ = null;
    }

  /**
   * @method request
   * @param {string}  method      Request method (GET, POST, PUT)
   * @param {string}  url         Request url (my-host/my-url/:id)
   * @param {any}     routeParams Values of url parameters
